    <string name="foreword">Пешгуфтор</string>
    <string name="title_foreword">Ободии миллат аз некӯаҳволии молиявии ҳар шаҳрванд вобаста аст</string>
    <string name="content1">Гуфтори 1</string>
    <string name="title_content1">Хулосаи таърихӣ аз Бобил</string>
    <string name="content2">Гуфтори 2</string>
    <string name="title_content2">Марде, ки орзӯи сарватманд шуданро дошт</string>
    <string name="content3">Гуфтори 3</string>
    <string name="title_content3">Сарватмандтарин марди Бобил</string>
    <string name="content4">Гуфтори 4</string>
    <string name="title_content4">Ҳафт роҳи ҳалли як ҳамёни холӣ</string>
    <string name="content5">Гуфтори 5</string>
    <string name="title_content5"><b>Қоидаи якум.</b> Шурӯъ ба пури пул кардани ҳамён</string>
    <string name="content6">Гуфтори 6</string>
    <string name="title_content6"><b>Қоидаи дувум.</b> Назорат кардани хароҷот</string>
    <string name="content7">Гуфтори 7</string>
    <string name="title_content7"><b>Қоидаи сеюм.</b> Сарватро зиёд кардан</string>
    <string name="content8">Гуфтори 8</string>
    <string name="title_content8"><b>Қоидаи ҷаҳорум ва панҷум.</b> Сарвати худро аз зиён эмин нигоҳ доред</string>
    <string name="content9">Гуфтори 9</string>
    <string name="title_content9"><b>Қоидаи шашум.</b> Хонаи худро ба як корхонаи фоидаовар табдил диҳед</string>
    <string name="content10">Гуфтори 10</string>
    <string name="title_content10"><b>Қоидаи ҳафтум.</b> Қобилияти ба даст оварданро зиёд кунед</string>
    <string name="content11">Гуфтори 11</string>
    <string name="title_content11">Бо олиҳаи мувафақ мулоқот кунед</string>
    <string name="content12">Гуфтори 12</string>
    <string name="title_content12"><b>Панҷ қонуни тилло</b></string>
    <string name="content13">Гуфтори 13</string>
    <string name="title_content13"><b>Қонуни аввали тилло.</b> Девори Бобил</string>
    <string name="content14">Гуфтори 14</string>
    <string name="title_content14"><b>Қонуни дувуми тилло.</b> Қарздиҳандаи тиллоии Бобил</string>
    <string name="content15">Гуфтори 15</string>
    <string name="title_content15"><b>Қонуни сеюми тилло.</b> Тилло ба муҳофизат ниёз дорад</string>
    <string name="content16">Гуфтори 16</string>
    <string name="title_content16"><b>Қонуни чоруми тилло.</b> Касе, ки тилло дорад вале дар медирияти он моҳир нест</string>
    <string name="content17">Гуфтори 17</string>
    <string name="title_content17"><b>Қонуни панҷуми тилло.</b> Тоҷири шутурони Бобил</string>
